{
  "name": "jira-cli",
  "description": "A JIRA Command Line Interface",
  "version": "2.1.0",
  "author": "Dan Shumaker <danpshumaker@gmail.com> (https://danshumaker.github.io/)",
  "contributors": [],
  "scripts": {
    "test": "jest",
    "test:e2e": "bin/tests.sh && exit 1",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "dev": "node bin/jira.js",
    "start": "npm i -g && jira",
    "clean:dev": "rimraf -rf ./.jira-cli",
    "clean": "rimraf -rf ~/.jira-cli && npm uninstall jira-cli -g",
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "alasql": "^0.6.0",
    "async": "^2.6.3",
    "cheerio": "^1.0.0-rc.3",
    "cli-table": "^0.2.0",
    "commander": "~1.1.1",
    "dargs": "^5.1.0",
    "inquirer": "^7.1.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "jira-connector": "^3.1.0",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.6",
    "openurl": "^1.1.0",
    "pad": "^3.2.0",
    "qs": "^6.9.4",
    "superagent": "~3.7.0",
    "underscore": "^1.10.2"
  },
  "engines": {
    "node": ">= 0.8"
  },
  "keywords": [
    "jira",
    "api",
    "client",
    "command",
    "cli",
    "tool",
    "command-line-junky"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/danshumaker/jira-cli.git"
  },
  "bin": {
    "jira": "./bin/jira.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^26.0.0",
    "rimraf": "^3.0.2"
  }
}
